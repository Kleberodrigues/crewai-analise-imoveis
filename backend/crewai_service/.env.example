# ============================================
# CONFIGURACAO DO PIPELINE DE LEILOES
# Copie para .env e preencha os valores
# ============================================

# OpenAI API
OPENAI_API_KEY=sk-your-openai-api-key-here
LLM_MODEL=gpt-4o

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6...

# Apify (Actor do Portal Zuk)
APIFY_TOKEN=apify_api_your-token-here
APIFY_ACTOR_ID=apify/web-scraper

# Diretorios
DATA_DIR=./data
OUTPUT_DIR=./output

# API
PORT=5000
DEBUG=false

# Notificacoes (opcional)
NOTIFY_WHATSAPP=5511999999999
TELEGRAM_BOT_TOKEN=your-telegram-bot-token
TELEGRAM_CHAT_ID=your-chat-id

# Timezone
TZ=America/Sao_Paulo
